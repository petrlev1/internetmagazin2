php_flag register_globals on
php_flag display_errors On

RewriteEngine on
Options -Indexes  +FollowSymlinks

RewriteBase /
RewriteRule ^.htaccess$ - [F]



RewriteRule ^odejda.htm$ /?page=odejda
RewriteRule ^obuv.htm$ /?page=obuv
RewriteRule ^aksessuari.htm$ /?page=aksessuari

RewriteRule ^([0-9]+).htm$ /?page=$1
RewriteRule ^([0-9]+)id([0-9]+).htm$ /?page=$1&pid=$2
